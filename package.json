{
  "name": "gltf-transform-generic-ext",
  "version": "0.1.0",
  "description": "A starter repo for creating libraries for browsers using TypeScript and Rollup.",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "new:pack": "npm run prepare && clean-package && npm pack && clean-package restore",
    "new:publish": "npm run prepare && clean-package && npm publish && clean-package restore",
    "build": "rimraf dist ; NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "serve-docs": "ws -d docs -p 8080",
    "serve": "ws -p 8000",
    "docs": "npx typedoc ./src/index.ts",
    "prepare": "npm run build && npm run docs"
  },
  "files": [
    "dist",
    "src",
    "docs",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/repalash/glTF-Transform-Generic-Ext.git"
  },
  "keywords": [
    "typescript",
    "util",
    "browser",
    "esm",
    "template",
    "javascript",
    "rollup",
    "starter",
    "library",
    "npm",
    "package",
    "module"
  ],
  "author": "repalash <palash@shaders.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/repalash/glTF-Transform-Generic-Ext/issues"
  },
  "homepage": "https://github.com/repalash/glTF-Transform-Generic-Ext#readme",
  "devDependencies": {
    "rimraf": "^5.0.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "clean-package": "^2.2.0",
    "local-web-server": "^5.2.1",
    "rollup": "^3.4.0",
    "rollup-plugin-license": "^3.0.1",
    "@gltf-transform/core": "^3.2.1",
    "@gltf-transform/extensions": "^3.2.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.24.7",
    "typescript": "^4.9.3"
  },
  "dependencies": {
  },
  "optionalDependencies": {
    "win-node-env": "^0.6.1"
  },
  "browserslist": [
    "defaults"
  ],
  "clean-package": {
    "remove": [
      "clean-package",
      "scripts",
      "devDependencies",
      "optionalDependencies",
      "//"
    ],
    "replace": {
      "main": "dist/bundle.min.js",
      "module": "dist/bundle.esm.js",
      "browser": "dist/bundle.umd.js",
      "types": "dist/index.d.ts"
    }
  }
}
